@page
@model BikeService.Pages.Users.DeleteModel
@{ ViewData["Title"] = "Delete";
                Layout = "~/Pages/Shared/_Layout.cshtml"; }


<br />
<h2 class="text-info">Delete Customer</h2>
<br />
<form method="post">
    <input asp-for="AplicationViewModel.Id" hidden />
    <div class=" border  backgroundwhite">
        <div class="form-group">
            <div class="row">
                <div class="col-sm-2">
                    <label asp-for="AplicationViewModel.Name" class="text-info"></label>
                </div>
                <div class="col-sm-5">
                    <input asp-for="AplicationViewModel.Name" disabled class="form-control" />
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-sm-2">
                    <label asp-for="AplicationViewModel.Email" class="text-info"></label>
                </div>
                <div class="col-sm-5">
                    <input asp-for="AplicationViewModel.Email" disabled class="form-control" />
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-sm-2">
                    <label asp-for="AplicationViewModel.PhoneNumber" class="text-info"></label>
                </div>
                <div class="col-sm-5">
                    <input asp-for="AplicationViewModel.PhoneNumber" disabled class="form-control" />
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-sm-2">
                    <label asp-for="AplicationViewModel.Address" class="text-info"></label>
                </div>
                <div class="col-sm-5">
                    <input asp-for="AplicationViewModel.Address" disabled class="form-control" />
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-sm-2">
                    <label asp-for="AplicationViewModel.City" class="text-info"></label>
                </div>
                <div class="col-sm-5">
                    <input asp-for="AplicationViewModel.City" disabled class="form-control" />
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-sm-2">
                    <label asp-for="AplicationViewModel.PostalCode" class="text-info"></label>
                </div>
                <div class="col-sm-5">
                    <input asp-for="AplicationViewModel.PostalCode" disabled class="form-control" />
                </div>
            </div>
        </div>

        <div class="form-group row">
            <div class="col-5 offset-2">
                <partial name="_DeleteAndBackToListPartial" />
            </div>
        </div>
    </div>
</form>

